{
	"name": "chriwo/mailhandler",
	"description": "Easy way to create email templates and use in other TYPO3 extensions",
	"keywords": [
		"TYPO3",
		"extension",
		"mail",
		"form"
	],
	"homepage": "https://github.com/chriwo/mailhandler",
	"authors": [
		{
			"name": "Christian Wolfram",
			"email": "c.wolfram@chriwo.de"
		}
	],
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^8.7 || ^9.5"
	},
	"replace": {
		"mailhandler": "self.version",
		"typo3-ter/mailhandler": "self.version"
	},
	"require-dev": {
		"nimut/testing-framework": "^1.0",
		"fzaninotto/faker": "^1.7"
	},
	"autoload": {
		"psr-4": {
			"ChriWo\\Mailhandler\\": "Classes/"
		}
	}
}
